{
  "name": "kanban",
  "version": "1.0.0",
  "description": "A kanban board written in React with Redux",
  "scripts": {
    "start": "node server.js",
    "dev": "yarn kill && NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "build": "rm -rf dist/ && NODE_ENV=production webpack --config webpack.config.prod.js",
    "test": "NODE_ENV=test NODE_PATH=./src jest --watch",
    "test:cov": "NODE_ENV=test NODE_PATH=./src jest --coverage",
    "kill": "lsof -Pni4 | grep ':8080\\|:3000' | awk '{print $2;}' | xargs kill",
    "lint": "eslint $(find src -name \\*.js)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/buraksaraloglu/kanban.git"
  },
  "author": "Burak Saraloglu <buraksaraloglu1@gmail.com> (http://github.com/buraksaraloglu)",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^7.3.0",
    "body-parser": "^1.19.1",
    "dotenv": "^8.4.0",
    "express": "^4.16.2",
    "express-history-api-fallback": "^2.2.1",
    "node-fetch": "^2.6.6",
    "normalize.css": "^8.0.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-icons": "^4.4.0",
    "sass": "^1.54.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@reduxjs/toolkit": "^1.3.6",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.8",
    "eslint": "^6.0.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^23.0.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-sort-class-members": "^1.3.1",
    "extract-css-chunks-webpack-plugin": "^4.0.1",
    "file-loader": "^6.0.0",
    "history": "^4.10.1",
    "html-webpack-inline-svg-plugin": "^2.3.0",
    "html-webpack-plugin": "4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-svg-loader": "^3.0.3",
    "redux": "^4.0.5",
